# Database Configuration
# Set to 'supabase' or 'postgresql'
VITE_DATABASE_TYPE=postgresql

# PostgreSQL Configuration (required if DATABASE_TYPE=postgresql)
VITE_PG_HOST=localhost
VITE_PG_PORT=5432
VITE_PG_USER=postgres
VITE_PG_PASSWORD=your_password
VITE_PG_DATABASE=your_database
VITE_JWT_SECRET=your_jwt_secret_key_min_32_chars

# Supabase Configuration (required if DATABASE_TYPE=supabase)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# GitHub OAuth (optional)
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_GITHUB_CLIENT_SECRET=your_github_client_secret